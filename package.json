{
  "name": "miniprogram-analysis-platform",
  "version": "1.0.0",
  "description": "code analysis for miniprogram",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "bull": "^3.20.0",
    "compressing": "^1.5.1",
    "egg": "^2.27.0",
    "egg-cors": "^2.2.3",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.11.0",
    "egg-view-ejs": "^2.0.1",
    "fs-extra": "^9.0.1",
    "miniprogram-analyzer2": "^0.0.8",
    "webpack-dev-server": "^3.1.11"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^7.10.0",
    "eslint-config-egg": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=miniprogram-analysis-platform",
    "stop": "egg-scripts stop --title=miniprogram-analysis-platform",
    "dev": "egg-bin dev",
    "stage": "egg-scripts start --daemon --env=stage --title=miniprogram-analysis-platform",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
